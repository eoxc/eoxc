{
  "name": "eoxc",
  "version": "1.1.0-rc.18",
  "description": "EOxC client generator",
  "main": "dist/eoxc.js",
  "scripts": {
    "build": "webpack --progress --config webpack.prod.js --profile --json > stats.json",
    "build-dev": "webpack --progress --config webpack.dev.js",
    "watch": "webpack --config webpack.dev.js --watch",
    "test": "mocha --compilers js:babel-register test/setup/node.js test/unit/**/*.js",
    "test:watch": "npm run test -- -w",
    "build-browser-tests": "webpack -d --config webpack.tests.config.js",
    "build-browser-tests:watch": "npm run build-browser-tests -- --watch --config webpack.tests.config.js",
    "dev-server": "webpack-dev-server --config webpack.dev.js --watch",
    "docs": "node_modules/.bin/jsdoc README.md src -d ./docs -r -P package.json -t node_modules/minami",
    "lint": "./node_modules/.bin/eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eoxc/eoxc.git"
  },
  "keywords": [],
  "author": "Fabian Schindler <fabian.schindler.strauss@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eoxc/eoxc/issues"
  },
  "homepage": "https://github.com/eoxc/eoxc",
